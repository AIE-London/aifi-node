{
  "files": ["./typescriptTest.ts"],
  "compilerOptions": {
    "incremental": false,
    "target": "es5",
    "module": "commonjs",
    "lib": ["esnext"],
    "rootDir": "./",
    "noEmit": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
  }
}
